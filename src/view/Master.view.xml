<mvc:View xmlns="sap.m" 
    xmlns:mvc="sap.ui.core.mvc" 
    displayBlock="true" 
    controllerName="webapp.typescript.controller.Master" 
    height="100%">
	<Page id="masterPage" title="Test category">
		<Table items="{/Data}">
			<columns>
				<Column><Label text="Master List" /></Column>
			</columns>
			<items>
				<ColumnListItem type="Navigation" vAlign="Middle" press="handleMasterPress">
					<cells>
						<Text text="{categoryName}"/>
					</cells>
				</ColumnListItem>								
			</items>
		</Table>
	</Page>
</mvc:View>


