<mvc:View xmlns="sap.m" 
    xmlns:mvc="sap.ui.core.mvc" 
    displayBlock="true" 
    controllerName="webapp.typescript.controller.Master" 
    height="100%">
	<Page id="masterPage" title="Categories" content="{/Data}">		
		<content>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" press="handleMasterPress" header="{categoryName}"
				frameType= "OneByOne">
				<TileContent />
			</GenericTile>
		</content>
	</Page>
</mvc:View>


