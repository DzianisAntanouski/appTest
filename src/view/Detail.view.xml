<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"  
    controllerName="webapp.typescript.controller.Detail" 
    height="100%">
	<Page id="detailPage" title="Detail page">
		<Table items="{subCategory}">
			<columns>
				<Column><Label text="Detail List" /></Column>
			</columns>
			<items>
				<ColumnListItem type="Inactive" vAlign="Middle" >
					<cells>
						<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" press="handleDetailPress" header="{name}"
							subheader="Subtitle"  frameType= "TwoByHalf">
							<TileContent />
						</GenericTile>
					</cells>
				</ColumnListItem>							
			</items>
		</Table>
		
	</Page>
</mvc:View>


