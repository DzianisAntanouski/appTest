<mvc:View xmlns="sap.m"
	xmlns:tnt="sap.tnt"
	xmlns:f="sap.f"
	xmlns:mvc="sap.ui.core.mvc" displayBlock="true" controllerName="webapp.typescript.controller.Start" height="100%">
	<Page class="sapUiTinyMargin" showHeader="false">
		<tnt:ToolHeader>
			<Text text="{i18n>testApplication}" wrapping="false" />
			<ToolbarSpacer />
			<Button text="{= ${supportModel>/auth} ? ${supportModel>/auth/email} : 'Not auth' }{}" type="Transparent" />
			<Avatar src="sap-icon://user" press='onPressAvatar' displaySize="XS" />
		</tnt:ToolHeader>
		<f:FlexibleColumnLayout id="fcl">
			<f:beginColumnPages>
				<mvc:XMLView id="beginView" async="true" viewName="webapp.typescript.view.Master"/>
			</f:beginColumnPages>
		</f:FlexibleColumnLayout>
	</Page>
</mvc:View>