<mvc:View xmlns="sap.m" xmlns:mvc="sap.ui.core.mvc" displayBlock="true"  
    controllerName="webapp.typescript.controller.DetailDetail" 
    height="100%">
	<Page id="detailDetailPage" title="DetailDetail page" backgroundDesign="Transparent">
		<headerContent>
			<Button type="Transparent" icon="sap-icon://decline" press="handleClose"  tooltip="Close end column" />
		</headerContent>
		<content>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" press="handleNavToManage" header="Manage test"
				subheader="Subtitle"  frameType= "TwoByHalf">
				<TileContent />
			</GenericTile>
			<GenericTile class="sapUiTinyMarginBegin sapUiTinyMarginTop tileLayout" press="handleNavToTest" header="Run test"
				subheader="Subtitle"  frameType= "TwoByHalf">
				<TileContent />
			</GenericTile>
		</content>
	</Page>
</mvc:View>


