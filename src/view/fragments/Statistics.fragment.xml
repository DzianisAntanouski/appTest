<core:FragmentDefinition xmlns="sap.m"
	xmlns:core="sap.ui.core"
	xmlns:table="sap.ui.table">
	<Dialog title="Statistic"  class="sapUiView">

		<table:Table rows="{supportModel>/results}" enableSelectAll="false" selectionMode="None">
			<table:columns>
				<table:Column>
					<Text text="Email" />
					<table:template>
						<Text text="{supportModel>email}" />
					</table:template>
				</table:Column>
				<table:Column>
					<Text text="Category" />
					<table:template>
						<Text text="{supportModel>category}" />
					</table:template>
				</table:Column>
				<table:Column>
					<Text text="Subcategory" />
					<table:template>
						<Text text="{supportModel>subcategory}" />
					</table:template>
				</table:Column>
				<table:Column>
					<Text text="Points" />
					<table:template>
						<Text text="{supportModel>points}" />
					</table:template>
				</table:Column>
			</table:columns>
			
		</table:Table>
		
		<buttons>
	
			<Button text="Cancel" press="onCancelFragmentResult" />
		</buttons>
	</Dialog>
</core:FragmentDefinition>